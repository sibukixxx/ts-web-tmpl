{
  "name": "src",
  "scripts": {
    "dev": "next dev",
    "vercel-build": "backend/infrastructure/db/prisma generate && next build",
    "build": "backend/infrastructure/db/prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:next": "next lint",
    "lint:all": "eslint . --max-warnings=0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check:types": "tsc --noEmit",
    "check:spell": "git diff --name-only | cspell --no-summary --no-progress --no-must-find-files --file-list stdin",
    "check:all": "concurrently \"pnpm lint\" \"pnpm check:types\" \"pnpm check:spell\"",
    "pre-commit": "concurrently \"lint-staged --allow-empty\" \"pnpm check:types\" \"pnpm check:spell\" \"pnpm format:check\"",
    "db:reset": "backend/infrastructure/db/prisma migrate reset --force",
    "db:migrate:dev": "backend/infrastructure/db/prisma migrate dev",
    "db:seed": "node --require ts-node/register backend/infrastructure/db/prisma/seed.ts",
    "db:reset:full": "sh scripts/reset-db.sh",
    "test": "vitest",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@hono/zod-openapi": "^0.18.3",
    "@hono/zod-validator": "^0.4.2",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.4",
    "@supabase/supabase-js": "^2.47.10",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "hono": "^4.6.15",
    "jose": "^5.9.6",
    "kysely": "^0.27.5",
    "lucide-react": "^0.469.0",
    "next": "15.1.1-canary.5",
    "qs": "^6.13.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "shadcn-ui": "^0.9.4",
    "stripe": "^16.12.0",
    "tailwindcss-animate": "^1.0.7",
    "ulid": "^2.3.0",
    "zod": "^3.24.1",
    "zod-openapi": "^4.2.2",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@faker-js/faker": "^9.3.0",
    "@hono/swagger-ui": "^0.5.0",
    "@prisma/client": "^6.1.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "4.0.0-beta.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.2",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "autoprefixer": "^10.4.20",
    "cspell": "^8.17.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^6.1.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "prisma": {
    "seed": "node --require ts-node/register prisma/seed.ts"
  },
  "pnpm": {
    "overrides": {
      "glob": "^10.4.5",
      "rimraf": "^3.0.2",
      "inflight": "^1.0.6"
    }
  }
}
